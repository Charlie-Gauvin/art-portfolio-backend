{
  "kind": "collectionType",
  "collectionName": "galleries",
  "info": {
    "singularName": "gallery",
    "pluralName": "galleries",
    "displayName": "gallery",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 50
    },
    "description": {
      "type": "text",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "artStatus": {
      "type": "enumeration",
      "enum": [
        "available",
        "sold"
      ],
      "required": true,
      "default": "available"
    },
    "dimensions": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "integer"
    }
  }
}
